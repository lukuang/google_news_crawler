{
  "manifest_version": 2,

  "name": "google crawler",
  "description": "This extension crawls google news result pages",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Click here!"
  },
   "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "activeTab",
    "alarms",
    "webRequest",
    "cookies", 
    "tabs", 
    "https://ajax.googleapis.com/",
    "https://www.google.com/*",
    "http://localhost:8080/*"
  ]

}